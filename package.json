{
  "name": "nodebb-plugin-s3-uploads",
  "version": "0.4.2",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nd-lqj/nodebb-plugin-s3-uploads"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "s3",
    "uploads"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.15.0"
  },
  "dependencies": {
    "aws-sdk": "^2.0.23",
    "uuid": "~1.4.1",
    "mime": "~1.2.11",
    "request": "~2.69.0",
    "gm": "~1.21.1"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "eslint": "8.5.0",
    "eslint-config-nodebb": "0.1.1",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.3"
  }
}
